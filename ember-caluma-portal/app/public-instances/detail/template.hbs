
<LoadingOrNotfound
  @loading={{this.fetchPublicInstance.isRunning}}
  @hasPermission={{this.publicInstance}}
>
  {{#if (has-feature "publication.form")}}
    <ul class="uk-tab">
      <BeSubnavLink @route="public-instances.detail.form">
        {{t "publicInstancesDetail.form"}}
      </BeSubnavLink>
      <BeSubnavLink @route="public-instances.detail.documents">
        {{t "publicInstancesDetail.documents"}}
      </BeSubnavLink>
    </ul>
  {{/if}}

  {{outlet}}
</LoadingOrNotfound>