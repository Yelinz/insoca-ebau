{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "XDebug",
      "type": "php",
      "request": "launch",
      "port": 9000,
      "pathMappings": {
        "/var/www/camac": "${workspaceRoot}/php"
      }
    },
    {
      "name": "Django",
      "type": "python",
      "request": "attach",
      "port": 5678,
      "host": "localhost",
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}/django",
          "remoteRoot": "."
        }
      ]
    }
  ]
}
