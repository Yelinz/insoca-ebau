<CamacForm @instance={{@model.instance}} @meta={{@model.meta}} as |form|>
  <form.table @identifier="bauherrschaft"/>
  {{#if this.questionActive}}
    <UkButton
      @on-click={{fn this.copyQuestionValue}}
      class="uk-align-center uk-margin-remove-vertical"
      @disabled={{unless this.grundeigentumerschaftValue "true"}}
    >
      Grundeigentümerschafts Personalien Übernehmen
    </UkButton>
  {{/if}}
</CamacForm>