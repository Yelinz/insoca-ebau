  },
  "art-der-nutzung": {
    "label": "Welchem Zweck dient das Vorhaben",
    "type": "multiselect",
    "required": true,
    "config": {
      "options": [
        "Wohnen",
        "Gewerbe / Dienstleistung",
        "Industrie",
        "Landwirtschaft",
        "Öffentliche Nutzung",
        "Forstwirtschaft",
        "Tourismus"
      ]
